@model MenuTableViewModel

@{
    ViewData["Title"] = "CreateMenuTable";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<div class="content">
	<div class="container-fluid">
		<h4 class="page-title">Yeni Hakkımızda Yazı Girişi</h4>
		<div class="row">
			<div class="col-md-12">
				<form asp-action="CreateMenuTable" method="post">
					<div class="card">
						<div class="card-header">
							<div class="card-title">Yeni Masa Girişi</div>
						</div>
						<div class="card-body">

							<div class="form-group">
								<label for="Name">Masa Adını Giriniz</label>
								<input type="text" class="form-control" id="Name" asp-for="Name">
							</div>

						</div>
						<div class="card-action">
							<button class="btn btn-primary">Masa Bilgisini Ekle</button>
						</div>
					</div>
				</form>
			</div>
		</div>
	</div>
</div>




